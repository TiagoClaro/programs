void setup(){

    Serial.begin(9600);

    pinMode(A0, INPUT);
    pinMode(A1, INPUT);

    display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
    display.clearDisplay();
    display.drawBitmap(0, 0, logo_ssd, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
    display.display();
    delay(2000);
    display.clearDisplay();
    display.display();
}

