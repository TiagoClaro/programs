float up, down, left, right, x, y, m, a;


void setup(){

    Serial.begin(9600);

    pinMode(A0, INPUT);
    pinMode(A1, INPUT);
    pinMode(A3, INPUT);
    pinMode(A2, INPUT);

    display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
    display.clearDisplay();
    display.drawBitmap(0, 0, logo_ssd, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
    display.display();
    delay(2000);
    display.clearDisplay();
    display.display();
}

void loop(){

    up = analogRead(A0);
    down = analogRead(A1);
    left = analogRead(A3);
    right = analogRead(A2);

    y = up - down;
    x = right - left;

    m = y / x;

    a = atan(m);
    a = (a * 180) / pi;

    if(x>0 && y>0){
        a = a;
    }
    else if(x<0 && y<0){
        a = a + 180;
    }
    else if(x>0 && y<0){
        a = a + 360;
    }
    else if(x<0 && y>0){
        a = a + 180;
    }

    arrow_angle(a);

}

void arrow_angle(float angle){

    if(angle == 0){
        display.drawBitmap(0, 0, graus_0, 128, 64, WHITE);
        display.display();
    }

    if(angle>0 && angle<=40){
        display.drawBitmap(0, 0, graus_40, 128, 64, WHITE);
        display.display();
    }

    if(angle>40 && angle<=80){
        display.drawBitmap(0, 0, graus_80, 128, 64, WHITE);
        display.display();
    }

    if(angle>80 && angle<=120){
        display.drawBitmap(0, 0, graus_120, 128, 64, WHITE);
        display.display();
    }

    if(angle>120 && angle<=160){
        display.drawBitmap(0, 0, graus_160, 128, 64, WHITE);
        display.display();
    }

    if(angle>160 && angle<=200){
        display.drawBitmap(0, 0, graus_200, 128, 64, WHITE);
        display.display();
    }

    if(angle>200 && angle<=240){
        display.drawBitmap(0, 0, graus_240, 128, 64, WHITE);
        display.display();
    }

    if(angle>240 && angle<=280){
        display.drawBitmap(0, 0, graus_280, 128, 64, WHITE);
        display.display();
    }

    if(angle>280 && angle<=320){
        display.drawBitmap(0, 0, graus_320, 128, 64, WHITE);
        display.display();
    }

    if(angle>320 && angle<=0){
        display.drawBitmap(0, 0, graus_0, 128, 64, WHITE);
        display.display();
    }

	display.clearDisplay();

}
